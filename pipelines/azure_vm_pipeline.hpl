        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>// To check value of the armSkuName for 'Standard' or 'Basic'

// Define the variable in string 'Standard' and 'Basic'
var isStandard = 'Standard';
var isBasic = 'Basic';

// if else statement to check for tier_type
if (armSkuName.match(isStandard) == isStandard){
	var tier_type = 'Standard';
} else if (armSkuName.match(isBasic) == isBasic) {
	var tier_type = 'Basic';
} else {
	var tier_type = 'Gateway';
};

// make a if else matching for versions
if (armSkuName.match('_v1') == '_v1'){
	var vm_version = 'v1';
} else if (armSkuName.match('_v2') == '_v2'){
	var vm_version = 'v2';
} else if (armSkuName.match('_v3') == '_v3'){
	var vm_version = 'v3';
} else if (armSkuName.match('_v4') == '_v4'){
	var vm_version = 'v4';
} else if (armSkuName.match('_v5') == '_v5'){
	var vm_version = 'v5';
} else if (armSkuName.match('_v6') == '_v6'){
	var vm_version = 'v6';
} else {
	var vm_version = 'Undefined';
};

</jsScript_script>
